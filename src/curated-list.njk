---
layout: layout.njk
pagination:
  data: data.lists
  size: 1
  alias: list
  addAllPagesToCollections: true
permalink: "{{ list.permalink }}/"
eleventyComputed:
  title: "{{ list.title }}"
---

<div class="curated-list-description">
  <p>{{ list.description }}</p>
</div>

<article>
{% set accordionData = {
  title: list.title,
  icon: list.icon,
  faqs: list.faqs
} %}
{% include "components/faq-accordion.njk" %}

