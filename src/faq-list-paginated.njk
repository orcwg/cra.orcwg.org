---
layout: layout.njk
pagination:
  data: data.lists
  size: 1
  alias: list
  addAllPagesToCollections: true
permalink: "{{ list.permalink }}"
eleventyComputed:
  title: "{{ list.title }}"
---

<section class="section-intro">
  <p>{{ list.description }}</p>
</section>

<article class="section-card">
{% set componentData = {
  title: list.title,
  icon: list.icon,
  items: list.items
} %}
{% include "components/faq-accordion.njk" %}
{% set item = list %}
{% include "components/actions.njk" %}
</article>
