---
layout: layout.njk
pagination:
  data: data.faqs
  size: 1
  alias: faq
permalink: "{{ faq.permalink }}"
eleventyComputed:
  title: "{{ faq.pageTitle }}"
---

<article class="section-card">
  <h1 class="section-header">{{ faq.question | markdownInline | safe }}</h1>
  {{ faq.answer | markdown | safe }}

  {% include "components/faq-warning.njk" %}
  
  {% include "components/actions.njk" %}
</article>
